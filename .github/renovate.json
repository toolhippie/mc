{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "github>toolhippie/.github//renovate/preset"
    ],
    "packageRules": [
        {
            "matchPackageNames": [
                "minio/mc"
            ],
            "versioning": {
                "type": "regex",
                "regex": "^RELEASE\\.(?<major>\\d{8})T(?<minor>\\d{2})(?<patch>\\d{2})(\\d{2})Z$"
            }
        }
    ]
}
